# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  ExerciseEngine_config = debug
endif
ifeq ($(config),release)
  ExerciseEngine_config = release
endif

PROJECTS := ExerciseEngine

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

ExerciseEngine:
ifneq (,$(ExerciseEngine_config))
	@echo "==== Building ExerciseEngine ($(ExerciseEngine_config)) ===="
	@${MAKE} --no-print-directory -C . -f ExerciseEngine.make config=$(ExerciseEngine_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f ExerciseEngine.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   ExerciseEngine"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"